<Window x:Class="GoldenTasksDesktop.Views.DashboardView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:vm="clr-namespace:GoldenTasksDesktop.ViewModels" 
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoldenTasksDesktop.Views"
        mc:Ignorable="d"
        Title="DashboardView" Height="450" Width="800">

    <Window.DataContext>
        <vm:UsuarioViewModel/>
    </Window.DataContext>

    <TextBlock Text="{Binding Nombre}" Width="100" Height="50"/>
</Window>
